programa
{

	funcao inicio()
	{
		inteiro y

			escreva("Digite um número: ")
			leia(y)
			
		para(inteiro x=1;x<=10;x++){
			escreva(y, " X ", x, " = ", x*y,"\n")
			
		}
	}
}
